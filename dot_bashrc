function gitp()
{
    unset PS1
    if [ -n "$EMACS" ]; then
        PS1="|Pr0mPT|\w|\w $(parse_git_branch) \u@\h%\n$ "
    else
        PS1="\w $(parse_git_branch) \u@\h%\n% "
    fi
}

