function e213()
{


  export MYPYTHON=$HOME/new37/envs/e213
  . $HOME/new37/etc/profile.d/conda.sh
  export PATH="$MYPYTHON/bin:$HOME/Applications/Visual Studio Code.app/Contents/Resources/app/bin:$PATH"
  conda activate e213
  condap
}

function a448()
{
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/phil/new37/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/phil/new37/etc/profile.d/conda.sh" ]; then
        . "/Users/phil/new37/etc/profile.d/conda.sh"
    else
        export PATH="/Users/phil/new37/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
export MYPYTHON=$HOME/new37/envs/a448
conda activate a448
condap
}


