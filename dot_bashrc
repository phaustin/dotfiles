function cd() { command cd "$@"; }
function pd() { command pushd "$@"; }
function s() { command pushd +"$@"; }
function dt() { command popd +"$@"; }
alias d='dirs'

function smallp()
{
    unset PS1
    if [ -n "$EMACS" ]; then
        PS1='|Pr0mPT|\w|\W \u@\h% '
    else
        PS1="\W \u@\h% "
    fi
}

function railp()
{
    unset PS1
    if [ -n "$EMACS" ]; then
        PS1='|Pr0mPT|\w|\W \u@\h% '
    else
        PS1="\W \u@rail "
    fi
}


function newp()
{
    unset PS1
    if [ -n "$EMACS" ]; then
        PS1="|Pr0mPT|\w|\w-$1> "
    else
        PS1="$1 \W> "
    fi
}

function condap()
{
    #the_name=$(hostname) -- Linux
    the_name=$(scutil --get LocalHostName)
    out=`basename $CONDA_PREFIX`
    unset PS1
    
    if [ -n "$EMACS" ]; then
        PS1='|Pr0mPT|\w|\w ${out} $(parse_git_branch) \u@${the_name}\n% '
    else
        PS1='\w ${out} $(parse_git_branch) \u@${the_name}\n% '
    fi
}


function oldp()
{
    unset PS1
    if [ -n "$EMACS" ]; then
        PS1='|Pr0mPT|\w|\w \u@\h% '
    else
        PS1="\w \u@\h% "
    fi
}

